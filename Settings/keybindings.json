// Place your key bindings in this file to override the defaults
[
  {
    "key": "shift+cmd+k j",
    "command": "atlascode.jira.showIssueForKey"
  },
  {
    "key": "shift+cmd+k g",
    "command": "launches.RunGameLocal",
    "arg": "restart"
  },
  {
    "key": "shift+cmd+k w",
    "command": "workbench.action.tasks.runTask",
    "args": "club: watch"
  },
  {
    "key": "shift+cmd+k s",
    "command": "launches.RunServer",
    "arg": "restart"
  },
  {
    "key": "shift+cmd+k m",
    "command": "workbench.action.tasks.runTask",
    "args": "club: generate model"
  },
  {
    "key": "shift+cmd+k u",
    "command": "workbench.action.tasks.runTask",
    "args": "club: update"
  },
  {
    "key": "shift+cmd+k k",
    "command": "workbench.action.tasks.runTask",
    "args": "club: nuke"
  },
  {
    "key": "shift+cmd+k g",
    "command": "workbench.action.tasks.runTask",
    "args": "club: generate"
  },
  {
    "key": "shift+cmd+k t",
    "command": "workbench.action.tasks.runTask",
    "args": "club: ci"
  },
  {
    "key": "shift+cmd+k p",
    "command": "workbench.action.tasks.runTask",
    "args": "club: prepare"
  },
  {
    "key": "shift+cmd+e",
    "command": "-workbench.view.explorer",
    "when": "viewContainer.workbench.view.explorer.enabled"
  },
  {
    "key": "cmd+r",
    "command": "workbench.action.debug.start",
    "when": "debuggersAvailable && debugState == 'inactive'"
  },
  {
    "key": "f5",
    "command": "-workbench.action.debug.start",
    "when": "debuggersAvailable && debugState == 'inactive'"
  },
  {
    "key": "cmd+.",
    "command": "workbench.action.debug.stop",
    "when": "inDebugMode && !focusedSessionIsAttach"
  },
  {
    "key": "shift+f5",
    "command": "-workbench.action.debug.stop",
    "when": "inDebugMode && !focusedSessionIsAttach"
  },
  {
    "key": "shift+cmd+x",
    "command": "editor.action.quickFix",
    "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "cmd+.",
    "command": "-editor.action.quickFix",
    "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+cmd+x",
    "command": "workbench.action.terminal.quickFix",
    "when": "terminalFocus && terminalProcessSupported"
  },
  {
    "key": "cmd+.",
    "command": "-workbench.action.terminal.quickFix",
    "when": "terminalFocus && terminalProcessSupported"
  },
  {
    "key": "shift+cmd+x",
    "command": "problems.action.showQuickFixes",
    "when": "problemFocus"
  },
  {
    "key": "cmd+.",
    "command": "-problems.action.showQuickFixes",
    "when": "problemFocus"
  },
  {
    "key": "ctrl+p",
    "command": "-workbench.action.quickOpenSelectPrevious",
    "when": "inQuickOpen"
  },
  {
    "key": "shift+cmd+o",
    "command": "workbench.action.quickOpen"
  },
  {
    "key": "cmd+p",
    "command": "-workbench.action.quickOpen"
  },
  {
    "key": "cmd+r",
    "command": "workbench.action.debug.restart",
    "when": "inDebugMode"
  },
  {
    "key": "shift+cmd+f5",
    "command": "-workbench.action.debug.restart",
    "when": "inDebugMode"
  },
  {
    "key": "shift+cmd+x",
    "command": "-workbench.view.extensions",
    "when": "viewContainer.workbench.view.extensions.enabled"
  },
  {
    "key": "ctrl+right",
    "command": "workbench.action.splitEditorToRightGroup"
  },
  // Explorer
  {
    "key": "cmd+1",
    "command": "workbench.view.explorer"
  },
  // Run and Debug
  {
    "key": "cmd+2",
    "command": "-workbench.action.focusSecondEditorGroup"
  },
  {
    "key": "cmd+2",
    "command": "workbench.view.debug",
    "when": "viewContainer.workbench.view.debug.enabled"
  },
  // Source Control
  {
    "key": "cmd+3",
    "command": "-workbench.action.focusThirdEditorGroup"
  },
  {
    "key": "cmd+3",
    "command": "workbench.view.scm",
    "when": "workbench.scm.active"
  },
  {
    "key": "cmd+3",
    "command": "workbench.view.scm",
    "when": "workbench.scm.active && !gitlens:disabled && config.gitlens.keymap == 'chorded'"
  },
  // Jira
  {
    "key": "cmd+4",
    "command": "-workbench.action.focusFourthEditorGroup"
  },
  {
    "key": "cmd+4",
    "command": "atlascode.views.jira.customJql.focus"
  },
  {
    "key": "cmd+'",
    "command": "editor.action.marker.nextInFiles",
    "when": "editorFocus"
  },
  {
    "key": "f8",
    "command": "-editor.action.marker.nextInFiles",
    "when": "editorFocus"
  },
  {
    "key": "cmd+b",
    "command": "-workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "cmd+b",
    "command": "workbench.action.tasks.build",
    "when": "taskCommandsRegistered"
  },
  {
    "key": "shift+cmd+b",
    "command": "-workbench.action.tasks.build",
    "when": "taskCommandsRegistered"
  },
  {
    "key": "shift+cmd+j",
    "command": "workbench.files.action.showActiveFileInExplorer"
  },
  {
    "key": "shift+cmd+w",
    "command": "workbench.action.closeEditorsAndGroup"
  },
  {
    "key": "shift+cmd+t",
    "command": "workbench.action.terminal.focus"
  },
  {
    "key": "shift+cmd+k",
    "command": "-editor.action.deleteLines",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "shift+alt+cmd+w",
    "command": "workbench.action.closeWindow",
    "when": "!editorIsOpen && !multipleEditorGroups"
  },
  {
    "key": "cmd+w",
    "command": "-workbench.action.closeWindow",
    "when": "!editorIsOpen && !multipleEditorGroups"
  },
  // Rename Symbol
  {
    "key": "shift+cmd+r",
    "command": "editor.action.rename",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "f2",
    "command": "-editor.action.rename",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+shift+g",
    "command": "-workbench.view.scm",
    "when": "workbench.scm.active"
  },
  {
    "key": "ctrl+shift+g",
    "command": "-workbench.view.scm",
    "when": "workbench.scm.active && !gitlens:disabled && config.gitlens.keymap == 'chorded'"
  },
  {
    "key": "shift+cmd+d",
    "command": "-workbench.view.debug",
    "when": "viewContainer.workbench.view.debug.enabled"
  },
  {
    "key": "f3",
    "command": "-editor.action.nextMatchFindAction",
    "when": "editorFocus"
  },
  {
    "key": "alt+/",
    "command": "-gitlens.gitCommands",
    "when": "!gitlens:disabled && config.gitlens.keymap == 'alternate'"
  },
  {
    "key": "shift+cmd+a",
    "command": "editor.action.changeAll",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "cmd+f2",
    "command": "-editor.action.changeAll",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+cmd+r",
    "command": "-rerunSearchEditorSearch",
    "when": "inSearchEditor"
  },
  {
    "key": "shift+cmd+a",
    "command": "-notification.acceptPrimaryAction",
    "when": "notificationToastsVisible"
  },
  {
    "key": "cmd+0",
    "command": "-workbench.action.focusSideBar"
  },
  {
    "key": "cmd+0",
    "command": "workbench.action.closeSidebar"
  },
  {
    "key": "shift+cmd+0",
    "command": "workbench.action.maximizeEditor"
  },
  {
    "key": "cmd+; cmd+f",
    "command": "-testing.debugCurrentFile",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+u",
    "command": "testing.debugCurrentFile"
  }
]