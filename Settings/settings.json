{
  "editor.minimap.enabled": false,
  "git.confirmSync": false,
  "git.autofetch": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "dart.openDevTools": "flutter",
  "workbench.startupEditor": "none",
  "workbench.sideBar.location": "right",
  "dart.debugExternalPackageLibraries": true,
  "dart.debugSdkLibraries": true,
  "explorer.confirmDragAndDrop": false,
  "git.suggestSmartCommit": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "pubspec.yaml": "pubspec.lock,pubspec_overrides.yaml,.packages,.flutter-plugins,.flutter-plugins-dependencies,.metadata",
    "*.dart": "${capture}.g.dart",
    "*.ts": "${capture}.typegen.ts",
    "*.tsx": "${capture}.typegen.ts",
    "*.mts": "${capture}.typegen.ts",
    "*.cts": "${capture}.typegen.ts"
  },
  "editor.fontLigatures": true,
  "editor.fontFamily": "'JetBrains Mono', Menlo, Monaco, 'Courier New', monospace",
  "explorer.confirmDelete": false,
  "workbench.iconTheme": "icons",
  "workbench.editor.openPositioning": "last",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.sharedViewState": true,
  "dart.renameFilesWithClasses": "prompt",
  "lldb.library": "~/.local/share/current-lldb",
  "window.restoreFullscreen": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "dart.showInspectorNotificationsForWidgetErrors": false,
  "yaml.schemas": {
    "file:///Users/sam/.vscode/extensions/atlassian.atlascode-3.0.2/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "redhat.telemetry.enabled": false,
  "go.survey.prompt": false,
  "go.toolsManagement.checkForUpdates": "off",
  "git.enableSmartCommit": true,
  "git.inputValidation": "off",
  "git.mergeEditor": true,
  "git.pruneOnFetch": true,
  "debug.toolBarLocation": "docked",
  "git.closeDiffOnOperation": true,
  "git.fetchOnPull": true,
  "git.postCommitCommand": "sync",
  "workbench.colorTheme": "GitHub Dark",
  "dart.checkForSdkUpdates": false,
  "githubPullRequests.pullBranch": "never",
  "git.ignoreRebaseWarning": true,
  "editor.indentSize": "tabSize",
  "editor.tabSize": 2,
  "editor.dragAndDrop": false,
  "atlascode.bitbucket.enabled": false,
  "dart.promptToGetPackages": false
}
